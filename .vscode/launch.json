  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/src/bin/www"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "current Program",
      "skipFiles": ["<node_internals>/**"],
      "args": ["--max-old-spaces=8192"],
      "program": "${file}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "asd Program",
      "skipFiles": ["<node_internals>/**"],
      "args": ["--max-old-spaces=8192"],
      "program": "${file}"
    }
  ]
}
